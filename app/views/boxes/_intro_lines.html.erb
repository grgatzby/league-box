<h3>
  <%= "Box #{box.box_number}" %>
  <%# if current_user.user_box_scores[0].box == @box %>
  <% if @this_is_my_box %>
    <%= render "fullname", user: current_user %>
  <% end %>
  <%= "(round #{box.round.start_date.strftime("%d %b")} - #{box.round.end_date.strftime("%d %b")})" %>
  <% if box == @my_current_box%>
    my current box
  <% end %>
</h3>
