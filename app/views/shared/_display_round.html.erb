<%# partial: display round start date and end date %>
<%# t(".round_period", start: round.start_date.strftime("%d %b"), end: round.end_date.strftime("%d %b %y")) %>
<%= t(@is_mobile ? ".round_period_mobile" : ".round_period",
    start: l(round.start_date, format: :ddmmm_date),
    end: l(round.end_date, format: @is_mobile ? :ddmmm_date : :ddmmmyy_date),
    round: @round_nb) %>
<% if round.end_date > Date.today %>
  <%= t(".days_left", days: (round.end_date - Date.today).to_i) %>
<% end %>
<%= t(".players", nb_players: round.boxes.map {|box| box.user_box_scores.count}.sum) %>&nbsp;
