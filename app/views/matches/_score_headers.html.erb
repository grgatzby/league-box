<div class="row">
  <%# sum of col- must equal 12 %>
  <div class="col-3"><%= t(".player") %></div>
  <div class="col-9 row">
    <div class="col-1"><%= t(".set_1") %></div>
    <div class="col-1"><%= t(".set_2") %></div>
    <% if tiebreak > 0 %>
      <div class="col-2"><%= t(".tie_break") %></div>
    <% end %>
    <div class="col-3"><%= t(".match_points") %></div>
    <div class="col-1"><%= t(".total") %></div>
  </div>
</div>
